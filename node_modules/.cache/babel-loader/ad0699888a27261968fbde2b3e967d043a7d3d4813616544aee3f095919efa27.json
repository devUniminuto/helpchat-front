{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { changePassword } from \"@/api/\";\nexport default {\n  data: () => ({\n    loading: false,\n    valid: true,\n    selection: 1,\n    user: null,\n    password: null,\n    rulesRequired: [v => !!v || \"Este campo es obligatorio\"],\n    msgError: \"\",\n    alertError: false,\n    alert: false,\n    msgAlert: \"\"\n  }),\n  methods: {\n    actualizarPassword() {\n      const validateForm = this.$refs.form.validate();\n      if (validateForm) {\n        this.loading = true;\n        this.alertError = false;\n        changePassword({\n          user: this.user\n        }).then(function (response) {\n          if (response.data[0].status == 1) {\n            this.loading = false;\n            this.alert = true;\n            this.msgAlert = \"Proceso realizado con éxito. Si el correo es correcto, va a recibir un correo con la nueva contraseña.\";\n          } else {\n            this.loading = false;\n            this.alertError = true;\n            this.msgError = \"Error de comunicación, comuníquese con el administrador.\";\n          }\n        }.bind(this)).catch(function (error) {\n          this.loading = false;\n          this.alertError = true;\n          this.msgError = \"Error al realizar solicitud, contáctese con el administrador.\";\n        }.bind(this));\n      }\n    },\n    registerHelp() {},\n    iniciarSesion() {\n      this.$router.push({\n        path: '/'\n      }).catch(() => {});\n    },\n    show_alert_and_fade() {\n      this.alertError = !this.alertError;\n    }\n  }\n};","map":{"version":3,"mappings":";AAqGA;AAEA;EACAA;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;EACA;EAEAC;IACAC;MACA;MACA;QACA;QACA;QACAC;UACAT;QACA,GACAU,KACA;UACA;YACA;YACA;YACA,gBACA;UACA;YACA;YACA;YACA,gBACA;UACA;QACA,aACA,CACAC,MACA;UACA;UACA;UACA,gBACA;QACA,aACA;MACA;IACA;IAEAC;IAEAC;MACA;QAAAC;MAAA;IACA;IAEAC;MACA;IACA;EACA;AACA","names":["data","loading","valid","selection","user","password","rulesRequired","msgError","alertError","alert","msgAlert","methods","actualizarPassword","changePassword","then","catch","registerHelp","iniciarSesion","path","show_alert_and_fade"],"sourceRoot":"src/components/static","sources":["RestorePassword.vue"],"sourcesContent":["<template>\n  <div class=\"mx-auto my-12 \">\n    <div class=\"logoEmpresaLogin shadowHover\">\n      <v-img width=\"100%\" src=\"@/assets/logo.png\"></v-img>\n    </div>\n\n    <v-card :loading=\"loading\" width=\"374\" outlined class=\"cardLogin shadowHover\">\n      <template slot=\"progress\">\n        <v-progress-linear\n          color=\"deep-purple\"\n          height=\"4\"\n          indeterminate\n        ></v-progress-linear>\n      </template>\n\n      <v-card-title class=\"titulo text-center\">Recuperar contraseña</v-card-title>\n\n      <v-card-text>\n        <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n          Ingrese su correo y si es válido, va a recibir una notificación con su nueva contraseña.\n          <v-container>\n            <v-row>\n              <v-col cols=\"12\" md=\"12\">\n                <v-text-field\n                  v-model=\"user\"\n                  label=\"Correo\"\n                  :rules=\"rulesRequired\"\n                  required\n                ></v-text-field>\n              </v-col>\n\n            </v-row>\n          </v-container>\n        </v-form>\n      </v-card-text>\n\n      <v-divider class=\"mx-4\"></v-divider>\n\n      <v-card-actions>\n        <v-row>\n          <v-col cols=\"12\" md=\"12\">\n            <v-btn color=\"lighten-2\" text @click=\"actualizarPassword\">\n              Cambiar contraseña\n            </v-btn>\n          </v-col>\n          <v-col cols=\"12\" md=\"6\">\n            <v-btn color=\"lighten-2\" text @click=\"iniciarSesion\">\n              Iniciar sesión\n            </v-btn>\n          </v-col>\n          <v-col cols=\"12\" md=\"6\">\n            <v-btn color=\"lighten-2\" text @click=\"registerHelp\">\n              ¿Desea <br />\n              registrarse?\n            </v-btn>\n          </v-col>\n        </v-row>\n      </v-card-actions>\n\n      <v-alert\n        :value=\"alertError\"\n        color=\"error\"\n        dark\n        border=\"top\"\n        icon=\"mdi-error\"\n        transition=\"slide-y-transition\"\n        style=\"z-index:1\"\n      >\n        <div class=\"pt-2 pr-6\">\n          <v-btn color=\"error\" fab x-small @click=\"show_alert_and_fade\">\n            <v-icon>mdi-close</v-icon>\n          </v-btn>\n\n          <br /><br />\n          {{ msgError }}\n        </div>\n      </v-alert>\n\n      <v-alert\n        :value=\"alert\"\n        color=\"lighten-2\"\n        dark\n        border=\"top\"\n        icon=\"mdi-error\"\n        transition=\"slide-y-transition\"\n        style=\"z-index:1\"\n      >\n        <div class=\"pt-2 pr-6\">\n          <v-btn color=\"lighten-2\" fab x-small @click=\"show_alert_and_fade\">\n            <v-icon>mdi-close</v-icon>\n          </v-btn>\n\n          <br /><br />\n          {{ msgAlert }}\n        </div>\n      </v-alert>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { changePassword } from \"@/api/\";\n\nexport default {\n  data: () => ({\n    loading: false,\n    valid: true,\n    selection: 1,\n    user: null,\n    password: null,\n    rulesRequired: [(v) => !!v || \"Este campo es obligatorio\"],\n    msgError: \"\",\n    alertError: false,\n    alert: false,\n    msgAlert: \"\"\n  }),\n\n  methods: {\n    actualizarPassword() {\n      const validateForm = this.$refs.form.validate();\n      if (validateForm) {\n        this.loading = true;\n        this.alertError = false;\n        changePassword({\n          user: this.user,\n        })\n          .then(\n            function (response) {\n              if (response.data[0].status == 1) {\n                this.loading = false;\n                this.alert = true;\n                this.msgAlert =\n                  \"Proceso realizado con éxito. Si el correo es correcto, va a recibir un correo con la nueva contraseña.\";\n              } else {\n                this.loading = false;\n                this.alertError = true;\n                this.msgError =\n                  \"Error de comunicación, comuníquese con el administrador.\";\n              }\n            }.bind(this)\n          )\n          .catch(\n            function (error) {\n              this.loading = false;\n              this.alertError = true;\n              this.msgError =\n                \"Error al realizar solicitud, contáctese con el administrador.\";\n            }.bind(this)\n          );\n      }\n    },\n\n    registerHelp(){},\n\n    iniciarSesion(){\n      this.$router.push({ path: '/'}).catch(()=>{});\n    },\n\n    show_alert_and_fade() {\n      this.alertError = !this.alertError;\n    },\n  },\n};\n</script>\n\n<style>\n.logoEmpresaLogin {\n  position: absolute;\n  width: 374px;\n  background-color: #fff;\n  padding: 20px;\n  border-radius: 90px;\n  z-index: 1;\n}\n\n.cardLogin {\n  margin-top: 130px;\n  border: 0 !important;\n  border-radius: 10px !important;\n  padding-top: 30px;\n}\n\n.cardLogin .titulo {\n  display: block !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}