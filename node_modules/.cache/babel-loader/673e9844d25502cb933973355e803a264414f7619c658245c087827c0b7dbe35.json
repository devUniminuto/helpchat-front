{"ast":null,"code":"import message from \"@/components/message.vue\";\nimport MiddleBox from \"@/components/static/MiddleBox.vue\";\nimport QuarterBox from \"@/components/static/QuarterBox.vue\";\nimport * as io from \"socket.io-client\";\nexport default {\n  data: () => ({\n    nombreChatSeleccionado: \"\",\n    chatsActivos: []\n  }),\n  methods: {\n    chatEntrante(data) {\n      console.log(data);\n    },\n    makeConnectionWS() {\n      this.socket = io.connect(\"ws://localhost:9000/\", {\n        cors: {\n          credentials: true\n        }\n      });\n      this.socket.on(\"client:newChatLive\", this.chatEntrante);\n    }\n  },\n  mounted() {\n    this.makeConnectionWS();\n  },\n  components: {\n    message,\n    MiddleBox,\n    QuarterBox\n  }\n};","map":{"version":3,"mappings":"AAsBA;AACA;AACA;AACA;AAGA;EACAA;IACAC;IACAC;EACA;EAEAC;IACAC;MACAC;IACA;IAEAC;MACA;QACAC;UACAC;QACA;MACA;MACA;IACA;EAEA;EAEAC;IACA;EACA;EAEAC;IACAC;IACAC;IACAC;EACA;AACA","names":["data","nombreChatSeleccionado","chatsActivos","methods","chatEntrante","console","makeConnectionWS","cors","credentials","mounted","components","message","MiddleBox","QuarterBox"],"sourceRoot":"src/views","sources":["ChatEnVivoView.vue"],"sourcesContent":["<template>\n  <div>\n    <QuarterBox class=\"shadowHover\">\n      <h2>Chats Activos</h2>\n      <div v-for=\"(chat, index) in chatsActivos\" :key=\"index\">\n        <message de=\"\" msg=\"Nuevo chat\" :receive=\"true\"></message>\n      </div>\n\n    </QuarterBox>\n    <MiddleBox class=\"shadowHover\">\n      <div\n        id=\"messages-container\"\n        style=\"overflow-y: scroll; height: 300px; padding: 10px\"\n      >\n        {{  }}\n        <message de=\"Bot\" msg=\"XD\" :receive=\"true\"></message>\n      </div>\n    </MiddleBox>\n  </div>\n</template>\n\n<script>\nimport message from \"@/components/message.vue\";\nimport MiddleBox from \"@/components/static/MiddleBox.vue\";\nimport QuarterBox from \"@/components/static/QuarterBox.vue\";\ni