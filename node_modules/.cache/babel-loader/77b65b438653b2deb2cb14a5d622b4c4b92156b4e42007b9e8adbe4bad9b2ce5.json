ttrs }\">\n        <v-btn color=\"primary\" v-bind=\"attrs\" v-on=\"on\">\n          Agregar\n        </v-btn>\n      </template>\n\n      <v-card>\n        <v-card-title class=\"text-h5 grey lighten-2\">\n          Crear un nuevo chat\n        </v-card-title>\n\n        <v-form>\n          <v-container>\n              <v-text-field\n                  v-model=\"nombreChat\"\n                  label=\"Nombre de nuevo chat\"\n                ></v-text-field>\n          </v-container>\n        </v-form>\n\n        <v-divider></v-divider>\n\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"primary\" text @click=\"createConversationalFlow()\"> Guardar </v-btn>\n          <v-btn color=\"primary\" text @click=\"dialog = false\"> Cancelar </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n\n    <v-data-table\n      :headers=\"headers\"\n      :items=\"chats\"\n      item-key=\"id\"\n      class=\"elevation-1\"\n      :search=\"search\"\n    >\n      <template v-slot:top>\n        <v-text-field\n          v-model=\"search\"\n          label=\"Buscar\"\n          class=\"mx-4\"\n        ></v-text-field>\n      </template>\n    </v-data-table>\n  </FullBoxVue>\n</template>\n\n<script>\nimport FullBoxVue from \"@/components/static/FullBox.vue\";\nimport { getAllConversationalFlowsMessages, createConversationalFlow } from \"@/api\";\n\nexport default {\n  data() {\n    return {\n      search: \"\",\n      chats: [],\n      headers: [\n        {\n          text: \"Id Conversación\",\n          value: \"id\",\n        },\n        {\n          text: \"Nombre\",\n          value: \"name\",\n        },\n      ],\n      dialog: false,\n      nombreChat: \"\",\n      infoFlujo: {}\n    };\n  },\n\n  methods: {\n    getConversationalFlows() {\n      getAllConversationalFlowsMessages().then(\n        function (response) {\n          let respuesta = response.data.response;\n          this.chats = respuesta.conversationalFlowMessages;\n          this.infoFlujo = respuesta.infoFlow\n        }.bind(this)\n      );\n    },\n\n    createConversationalFlow() {\n      createConversationalFlow({\"name\": this.nombreChat}).then(\n        function (response) {\n          this.getAllConversationalFlows()\n          this.nombreChat = \"\";\n          this.dialog = false;\n        }.bind(this)\n      )\n    },\n  },\n\n  mounted: function () {\n    this.getAllConversationalFlows();\n  },\n\n  components: {\n    FullBoxVue,\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}