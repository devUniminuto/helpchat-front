{"ast":null,"code":"import VueJwtDecode from 'vue-jwt-decode';\nexport default {\n  name: 'HelloWorld',\n  data: () => ({\n    nombreUser: null,\n    importantLinks: [{\n      text: 'Página principal',\n      href: 'https://www.colpryst.com/'\n    }]\n  }),\n  methods: {\n    validLogin() {\n      if (localStorage.getItem(\"userTkn\") === null) {\n        this.validUser = false;\n      } else {\n        this.nombreUser = VueJwtDecode.decode(localStorage.getItem(\"userTkn\")).nombre;\n      }\n    }\n  },\n  mounted: function () {\n    this.validLogin();\n  }\n};","map":{"version":3,"mappings":"AAiDA;AAEA;EACAA;EAEAC;IACAC;IACAC,iBACA;MACAC;MACAC;IACA;EAEA;EACAC;IACAC;MACA;QACA;MACA;QACA;MACA;IACA;EACA;EACAC;IACA;EACA;AACA","names":["name","data","nombreUser","importantLinks","text","href","methods","validLogin","mounted"],"sourceRoot":"src/components","sources":["HelloWorld.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row class=\"text-center\">\n      <v-col cols=\"12\">\n        <v-img\n          :src=\"require('../assets/logo.png')\"\n          class=\"my-1\"\n          contain\n          height=\"100\"\n        />\n      </v-col>\n\n      <v-col class=\"mb-4\">\n        <h1 class=\"display-2 font-weight-bold mb-3\">\n          ¡Bienvenido/a {{ nombreUser }}!\n        </h1>\n\n        <!-- <p class=\"subheading font-weight-regular\">\n          COLPRYST - Estudios de confiabilidad\n\n        </p> -->\n      </v-col>\n\n      <v-col\n        class=\"mb-5\"\n        cols=\"12\"\n      >\n        <h2 class=\"headline font-weight-bold mb-3\">\n          Enlaces que le pueden interesar\n        </h2>\n\n        <v-row justify=\"center\">\n          <a\n            v-for=\"(link, i) in importantLinks\"\n            :key=\"i\"\n            :href=\"link.href\"\n            class=\"subheading mx-3\"\n            target=\"_blank\"\n          >\n            {{ link.text }}\n          </a>\n        </v-row>\n      </v-col>\n\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport VueJwtDecode from 'vue-jwt-decode'\n\nexport default {\n  name: 'HelloWorld',\n\n  data: () => ({\n    nombreUser: null,\n    importantLinks: [\n      {\n        text: 'Página principal',\n        href: 'https://www.colpryst.com/'\n      }\n    ]\n  }),\n  methods: {\n    validLogin() {\n      if (localStorage.getItem(\"userTkn\") === null) {\n        this.validUser = false;\n      } else {\n        this.nombreUser = VueJwtDecode.decode(localStorage.getItem(\"userTkn\")).nombre\n      }\n    },\n  },\n  mounted: function () {\n    this.validLogin();\n  },\n}\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}